{
  "name": "bombstats-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm:cli": "ts-node -r tsconfig-paths/register ./node_modules/.bin/typeorm",
    "db:migration:create": "yarn typeorm:cli migration:create",
    "db:migration:generate": "yarn typeorm:cli migration:generate -d ./src/database/data-source.ts",
    "db:migration:run": "yarn typeorm:cli migration:run -d ./src/database/data-source.ts",
    "db:migration:generate:entity": "yarn typeorm-model-generator -h localhost -p 3306 -d bombstats -u root -x your_password -e mysql -o output_directory",
    "db:migration:revert": "yarn typeorm:cli migration:revert -d ./src/database/data-source.ts",
    "sentry:sourcemaps": "sentry-cli sourcemaps inject --org lucas-45 --project node-nestjs ./dist && sentry-cli sourcemaps upload --org lucas-45 --project node-nestjs ./dist"
  },
  "dependencies": {
    "@bull-board/api": "^5.21.4",
    "@bull-board/express": "^5.21.4",
    "@bull-board/nestjs": "^5.21.4",
    "@nestjs/bull": "^10.1.1",
    "@nestjs/bullmq": "^10.1.1",
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^10.3.9",
    "@nestjs/schedule": "^4.0.2",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.3.9",
    "@opensea/stream-js": "^0.1.3",
    "@pm2/io": "^6.0.1",
    "@sentry/cli": "^2.32.1",
    "@sentry/nestjs": "^8.15.0",
    "@sentry/profiling-node": "^8.15.0",
    "alchemy-sdk": "^3.3.1",
    "axios": "^1.7.2",
    "bullmq": "^5.8.4",
    "cheerio": "^1.0.0-rc.12",
    "date-fns": "^3.6.0",
    "ethers": "6.9.0",
    "firebase-admin": "^12.2.0",
    "free-proxy": "^0.2.1",
    "https-proxy-agent": "^7.0.5",
    "ioredis": "^5.4.1",
    "mysql2": "^3.10.1",
    "opensea-js": "^7.1.11",
    "ping": "^0.4.4",
    "proxy-lists": "^1.23.3",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "sfs2x-api": "^1.8.1",
    "socket.io": "^4.7.5",
    "socket.io-client": "^4.7.5",
    "typeorm": "^0.3.20",
    "web3": "^4.10.0",
    "web3-validator": "^2.0.6",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typeorm-model-generator": "^0.4.6",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
